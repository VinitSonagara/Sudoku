.eg{
    background:black;
}

.mainBox{
    width:100%;
    heigth:495px;
}

.row{
    width:100%;
    height:55px;
}

.dataBox{
    border:1px solid black;
    height:55px;
    width:11.1%;
    padding-top: 15px;
    text-align: center;
    font-size:2em;
    float:left;
    cursor:pointer;
}

.notesOuterBox{
    border:1px solid black;
    height:55px;
    width:11.1%;
    float:left;
}

.notesBox{
    border:1px solid black;
    height:16.7px;
    width:33%;
    float:left;
    padding:0;
    text-align: center;

    // To remove cursor
    color: transparent;
    text-shadow: 0 0 0 #2196f3;

    &:focus {
        outline: none;
    }
}

.permData{
    color:red;
}

input{
    height:55px;
    width:100%;
    outline:none;
    border:hidden;
    padding-bottom:15px;
    cursor:pointer;
}

/* Chrome, Safari, Edge, Opera */
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
  -webkit-appearance: none;
  margin: 0;
}

/* Firefox */
input[type=number] {
  -moz-appearance: textfield;
}

